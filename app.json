{
  "expo": {
    "name": "Smartbot",
    "slug": "smartbot",
    "description": "AI assistants from several different providers in one app. Switch between providers and their models without starting a new chat.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundImage": "./assets/icon.png"
      },
      "package": "com.jorgenlt.smartbot"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "89ebe46f-f502-4fa0-a930-0c4c5273d088"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#A5FF99",
          "image": "./assets/icon.png",
          "dark": {
            "image": "./assets/icon.png",
            "backgroundColor": "#A5FF99"
          },
          "imageWidth": 200
        }
      ]
    ]
  }
}
